<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:minHeight="1000dp"
    android:minWidth="1000dp"
    tools:context="com.example.appairbnb.MainActivity">

    <LinearLayout
        android:layout_width="359dp"
        android:layout_height="592dp"
        android:layout_margin="0dp"
        android:layout_marginBottom="0dp"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="0dp"
        android:layout_marginTop="0dp"
        android:orientation="vertical"
        android:weightSum="1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        tools:layout_editor_absoluteX="15dp"
        tools:layout_editor_absoluteY="-7dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="80dp"
            android:orientation="horizontal"
            android:weightSum="1">

            <ImageView
                android:id="@+id/imageView2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="0dp"
                android:background="#006E7B"
                android:cropToPadding="false"
                android:onClick="mainclick"
                android:paddingLeft="10dp"
                android:paddingRight="10dp"
                app:srcCompat="@drawable/drawable13" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:background="#006F7C"
            android:orientation="horizontal"
            android:weightSum="1">

            <ImageButton
                android:id="@+id/imageButton"
                android:layout_width="90dp"
                android:layout_height="match_parent"
                android:layout_weight="0.01"
                android:paddingLeft="10dp"
                app:srcCompat="@drawable/drawable14" />

            <ImageButton
                android:id="@+id/imageButton2"
                android:layout_width="90dp"
                android:layout_height="match_parent"
                android:background="#106f70"
                android:onClick="mainclick"
                app:srcCompat="@drawable/drawable15" />

            <ImageView
                android:id="@+id/imageView16"
                android:layout_width="212dp"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/drawable40" />

        </LinearLayout>

        <TextView
            android:id="@+id/textView"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:paddingBottom="20dp"
            android:paddingLeft="20dp"
            android:paddingTop="20dp"
            android:text="Just booked"
            android:textSize="18sp" />

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="290dp"
            android:orientation="horizontal"
            android:weightSum="1">

            <LinearLayout
                android:layout_width="184dp"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:weightSum="1">

                <ImageView
                    android:id="@+id/imageView5"
                    android:layout_width="168dp"
                    android:layout_height="wrap_content"
                    android:paddingLeft="40dp"
                    android:scaleType="center"
                    app:srcCompat="@drawable/drawable18" />

                <TextView
                    android:id="@+id/textView3"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.07"
                    android:paddingLeft="25dp"
                    android:text="$25 Hike the"
                    android:textSize="18sp" />

                <TextView
                    android:id="@+id/textView4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingLeft="25dp"
                    android:text="Hollywood Hills and..."
                    android:textSize="16sp" />

                <ImageView
                    android:id="@+id/imageView6"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingLeft="25dp"
                    app:srcCompat="@drawable/drawable20" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="184dp"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:weightSum="1">

                <ImageView
                    android:id="@+id/imageView9"
                    android:layout_width="163dp"
                    android:layout_height="wrap_content"
                    android:paddingLeft="40dp"
                    android:scaleType="center"
                    app:srcCompat="@drawable/drawable19" />

                <TextView
                    android:id="@+id/textView5"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:paddingLeft="10dp"
                    android:text="$68 Prepare a Korean"
                    android:textSize="18sp" />

                <TextView
                    android:id="@+id/textView6"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingLeft="10dp"
                    android:text="meal with Yang Chul'..."
                    android:textSize="16sp" />

                <ImageView
                    android:id="@+id/imageView10"
                    android:layout_width="163dp"
                    android:layout_height="wrap_content"
                    app:srcCompat="@drawable/drawable21" />

            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="0.35"
            android:orientation="horizontal"
            android:weightSum="1">

            <TextView
                android:id="@+id/textView7"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:paddingTop="5dp"
                android:text="  Experiences"
                android:textSize="18sp"
                android:textStyle="bold" />

            <ImageView
                android:id="@+id/imageView12"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:paddingRight="20dp"
                app:srcCompat="@drawable/drawable28" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="0.09"
            android:orientation="horizontal">

            <ImageButton
                android:id="@+id/imageButton3"
                android:layout_width="70dp"
                android:layout_height="68dp"
                android:background="@android:color/background_light"
                app:srcCompat="@drawable/drawable23" />

            <ImageButton
                android:id="@+id/imageButton4"
                android:layout_width="70dp"
                android:layout_height="75dp"
                android:layout_weight="1"
                android:background="@android:color/background_light"
                android:onClick="main2click"
                app:srcCompat="@drawable/drawable24" />

            <ImageButton
                android:id="@+id/imageButton5"
                android:layout_width="65dp"
                android:layout_height="75dp"
                android:layout_weight="1"
                android:background="@android:color/background_light"
                app:srcCompat="@drawable/drawable25" />

            <ImageButton
                android:id="@+id/imageButton6"
                android:layout_width="65dp"
                android:layout_height="75dp"
                android:layout_weight="1"
                android:background="@android:color/background_light"
                app:srcCompat="@drawable/drawable26" />

            <ImageButton
                android:id="@+id/imageButton7"
                android:layout_width="65dp"
                android:layout_height="75dp"
                android:background="@android:color/background_light"
                app:srcCompat="@drawable/drawable27" />
        </LinearLayout>

    </LinearLayout>
</android.support.constraint.ConstraintLayout>
